# Lab 1 -- Shanelle
# Set working directory (relative path)
setwd("spring26/data analytics/lab1/")
# Read data
epi.data <- read_csv("epi_results_2024_pop_gdp.csv")
# View dataframe
View(epi.data)
# Choosing the variables population and GDP
epi.data$population
epi.data$gdp
# Print summary of variables
summary(epi.data$population)
summary(epi.data$gdp)
# Store in a separate variable and remove NAs
GDP <- epi.data$gdp
POP <- epi.data$population
G_NAs <- is.na(GDP)
GDP.complete <- GDP[!G_NAs]
P_NAs <- is.na(POP)
POP.complete <- POP[!P_NAs]
# Boxplot of variables
boxplot(GDP.complete, POP.complete, names = c("GDP","Population"))
# Histogram w/ overlayed prob dist
# Histogram for GDP
hist(GDP.complete, prob=TRUE)
# Changed step size '5' to 'length.out=100' to prevent memory hang
x1 <- seq(min(GDP.complete), max(GDP.complete), length.out=100)
d1 <- dnorm(x1, mean=mean(GDP.complete), sd=sd(GDP.complete), log=FALSE)
lines(x1, d1, col="blue", lwd=2)
# Histogram for Population
hist(POP.complete, prob=TRUE)
# Changed step size '5' to 'length.out=100' to handle the large scale of population
x2 <- seq(min(POP.complete), max(POP.complete), length.out=100)
d2 <- dnorm(x2, mean=mean(POP.complete), sd=sd(POP.complete), log=FALSE)
lines(x2, d2, col="red", lwd=2)
# Plot ecdfs
plot(ecdf(GDP.complete), do.points=FALSE, verticals=TRUE)
plot(ecdf(POP.complete), do.points=FALSE, verticals=TRUE)
# Quantile-quantile Plots
qqnorm(GDP.complete); qqline(GDP.complete)
qqnorm(POP.complete); qqline(POP.complete)
# print quantile-quantile plot for 2 variables
qqplot(GDP.complete, POP.complete, xlab = "Q-Q plot for EPI.new & EPI.old")
# Statistical Tests
shapiro.test(GDP.complete)
shapiro.test(POP.complete)
ad.test(GDP.complete)
ad.test(POP.complete)
ks.test(GDP.complete,POP.complete)
